{
  "version": 2,
  "functions": {
    "backend/server-minimal.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server-minimal.js"
    },
    {
      "src": "/(.*)",
      "dest": "/backend/server-minimal.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
